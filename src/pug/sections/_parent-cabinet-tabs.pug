.parent-cabinet__visits.parent-cabinet__tab(aria-label="Вкладка Посещения" id="visits")
    .parent-cabinet__tab-wrapper
        .calendar-container
            .calendar-header
                h3
                    | Октябрь 2020 г.

                .calendar-header__buttons
                    button.calendar-header__button.calendar-header__button--backward Назад
                    span Сегодня
                    button.calendar-header__button.calendar-header__button--forward Вперед
            .calendar
                - for(i=0;i<7;i++)
                    span.day-name= names[i]

                - for(j=0;j<35;j++)
                    - if(j == 0 || j == 1 || j == 33 || j == 34)
                        div.day.day--disabled= days[j]

                    - else
                        div.day= days[j]

                section.task.task--primary(style="grid-row: 2") Projects
                section.task.task--primary Вильгельма Пика
                    .task__detail
                        h4 Product Checkup 1
                        p 15-17th November
                section.task.task--primary(style="margin-bottom: 50px") Вильгельма Пика